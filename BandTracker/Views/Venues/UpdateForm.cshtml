@{
  Layout = "_Layout";
}
<h1>Band Tracker</h1>
<hr />
<h2>Edit Venue: @Model.GetName()</h2>
<form action="/venues/@Model.GetId()/update" method="post">
  <label for="new-venue-name">Name: </label>
  <input type="text" name="new-venue-name" value="@Model.GetName()">
  <button type="submit">Edit Venue</button>
</form>
<h4><a href="/">Return Home</a><h4>
